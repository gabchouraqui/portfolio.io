   "cell_type": "code",
   "execution_count": 29,
   "id": "8357601f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Original text: \"Python\", \"PHP\", \"Java\"\n",
      "Extracted values: ['Python', 'PHP', 'Java']\n"
     ]
    }
   ],
   "source": [
    "def extract_values_between_quotes(input_text):\n",
    "    # Define a regular expression pattern to match values between quotation marks\n",
    "    pattern = r'\"([^\"]+)\"'\n",
    "    \n",
    "    # Use re.findall to find all matches in the input text\n",
    "    values = re.findall(pattern, input_text)\n",
    "    \n",
    "    return values\n",
    "\n",
    "text1 = '\"Python\", \"PHP\", \"Java\"'\n",
    "extracted_values = extract_values_between_quotes(text1)\n",
    "\n",
    "print(f\"Original text: {text1}\")\n",
    "print(f\"Extracted values: {extracted_values}\")"
   ]
